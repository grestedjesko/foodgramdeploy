# RabbitMQ Connection (из Vault в Kubernetes)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=foodgram_user
RABBITMQ_PASSWORD=change_me_secure_password

# RabbitMQ Exchange
RABBITMQ_EXCHANGE=foodgram_tasks
RABBITMQ_EXCHANGE_TYPE=direct

# API Keys для внешних API
# TheMealDB: используйте '1' для бесплатной версии или получите ключ на https://www.themealdb.com/api.php
THEMEALDB_API_KEY=1

# Open Food Facts не требует API ключа
# OPENFOODFACTS_API_KEY=

# Директория для сохранения результатов
API_RESULTS_DIR=api_results
